{
  "version": "0.0.1",
  "imports": {
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.4.3",
    "hono": "npm:hono@^4.7.4",
    "zod": "npm:zod@^3.24.2"
  },
  "tasks": {
    "dev": "deno run --watch --allow-net src/main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": [
      "./src",
      "./tools"
    ],
    "exclude": []
  },
  "nodeModulesDir": "auto"
}