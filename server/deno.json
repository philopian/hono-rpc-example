{
  "version": "0.0.1",
  "imports": {
    "hono": "jsr:@hono/hono@^4.7.4"
  },
  "tasks": {
    "dev": "deno run --watch --allow-net src/main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": ["./src", "./tools"],
    "exclude": []
  }
}
